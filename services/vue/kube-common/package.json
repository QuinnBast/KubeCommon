{
  "name": "kube-common",
  "version": "0.0.0",
  "description": "Conquer the Kubernetes world",
  "private": true,
  "type": "module",
  "main": "electron/main.js",
  "homepage": "./",
  "author": "Quinn Bast",
  "build": {
    "appId": "com.queuebee.software.kubecommon",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "**/*"
    ]
  },
  "scripts": {
    "electron": "npm run build && electron .",
    "tsc": "tsc --build tsconfig.node.json",
    "dev": "vite",
    "build": "eslint . && vite build --emptyOutDir",
    "release": "npm run build && electron-builder",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "devtools": "vue-devtools",
    "clean": "rm -rf ./dist",
    "lint": "eslint ."
  },
  "dependencies": {
    "@kubernetes/client-node": "1.0.0",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.17.6",
    "clipboard": "^2.0.11",
    "moment": "^2.30.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-json-viewer": "^2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^22.13.14",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^35.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
