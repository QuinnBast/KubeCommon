{
  "name": "kube-common",
  "version": "0.0.0",
  "description": "Conquer the Kubernetes world",
  "private": true,
  "type": "module",
  "main": "electron/main.js",
  "homepage": "./",
  "author": "Quinn Bast",
  "build": {
    "appId": "com.queuebee.software.kubecommon",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "**/*"
    ]
  },
  "scripts": {
    "electron": "vite build --emptyOutDir && electron .",
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "release": "vite build --emptyOutDir && electron-builder",
    "preview": "vite preview",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@kubernetes/client-node": "1.0.0",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.17.6",
    "clipboard": "^2.0.11",
    "moment": "^2.30.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-json-viewer": "^2.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^35.1.2",
    "electron-builder": "^26.0.12",
    "prettier": "3.5.3",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2"
  }
}
